{
    "name": "fredattack/support-tickets-notifications",
    "description": "This is my package support-tickets-notifications",
    "keywords": [
        "fredattack",
        "laravel",
        "support-tickets-notifications",
        "slack",
        "sms"
    ],
    "homepage": "https://github.com/fredattack/support-tickets-notifications.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Moras",
            "email": "fred.moras@addf.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "bensampo/laravel-enum": "^3.4",
        "illuminate/contracts": "^8.37",
        "laravel/slack-notification-channel": "^2.3",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "spatie/laravel-ray": "^1.25.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Fredattack\\SupportTicketsNotifications\\": "src",
            "Fredattack\\SupportTicketsNotifications\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fredattack\\SupportTicketsNotifications\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fredattack\\SupportTicketsNotifications\\SupportTicketsNotificationsServiceProvider",
                "Fredattack\\SupportTicketsNotifications\\EventServiceProvider"
            ],
            "aliases": {
                "SupportTicketsNotifications": "Fredattack\\SupportTicketsNotifications\\SupportTicketsNotificationsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
